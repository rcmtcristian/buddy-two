{
  "name": "next-ts-eslint-prettier-husky-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@react-three/drei": "^9.66.4",
    "@react-three/fiber": "^8.13.0",
    "@types/node": "^18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "clsx": "^1.2.1",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "firebase": "^9.15.0",
    "lodash": "^4.17.21",
    "next": "13.1.1",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/three": "^0.152.0",
    "eslint-config-prettier": "^8.5.0",
    "fs": "^0.0.1-security",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "three": "^0.152.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@3.3.1"
